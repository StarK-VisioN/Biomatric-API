<!DOCTYPE html>
<html lang="en">
<head>
    <title>Attendance Records</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <div class="container">
        <h1>Attendance Records</h1>
        <table>
            <thead>
                <tr>
                    <th>Employee ID</th>
                    <th>Timestamp</th>
                    <th>Status</th>
                </tr>
            </thead>
            <tbody>
                <% attendance.forEach(record => { %>
                <tr>
                    <td><%= record.employeeId %></td>
                    <td><%= record.timestamp %></td>
                    <td><%= record.status %></td>
                </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
</body>
</html>
